#myChart{
    display: block;
    box-sizing: border-box;
    height: 472px;
    width: 944px;
}
</style>

@* table cdn *@

<section id="content-wrapper">
    <div class="container-fluid">
        <div class="reports-wrapper">
            <div class="row size">
                <div class="col-md-3">
                    <!-- Tabs nav -->
                    <div class="nav flex-column nav-pills nav-pills-custom"
                        id="v-pills-tab" role="tablist"
                        aria-orientation="vertical">
                    </div>
                </div>
                <div class="col-md-9">
                    <!-- Tabs content -->
                    <div class="tab-content" id="v-pills-tabContent">
                        <!-- charts bar-->
                        <div
                            class="tab-pane fade shadow rounded bg-white show active p-5"
                            id="v-pills-Indent_Status"
                            role="tabpanel"
                            aria-labelledby="v-pills-home-tab">
                            <h4 class="font-italic mb-4">Reports</h4>
                            <div class="tab-content" id="v-pills-tabContent">
                                <div class="chartbox" style="width: 90%;">
                                    @* <canvas id="myChart"></canvas> *@
                                    <canvas id="myChart" width="944"
                                        height="472"></canvas>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<div class="spinnerbody" id="spinnerbody">
    <div class="spinnerchild">
        <div class="spinner-border text-success " role="status">
            <span class="sr-only"></span>
        </div>
        <span>Loading....</span>
    </div>
</div>

<script>
    window.ApiUrl1 = '@Model.ApiUrl1';
    window.ApiUrl2 = '@Model.ApiUrl2';
    window.BaseUrl = '@Model.BaseUrl';
    window.Port1 = '@Model.Port1';
    window.Port2 = '@Model.Port2';
    window.Endpoint1 = '@Model.Endpoint1';
    window.Endpoint2 = '@Model.Endpoint2';
</script>

